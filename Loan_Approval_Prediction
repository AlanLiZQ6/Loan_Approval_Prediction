ame='STACK', submission=False):\n    \n    model = ClassifierWrapper(LogisticRegression(penalty='l2',  C=10, max_iter=5000, tol=1e-6), name=name)\n    cv_data = model.cv(\n        train[members + [TARGET]], \n        test[members], \n        dataset_name=str(members), \n        n_splits=10,\n        n_repeats=1,\n        verbose=verbose,\n        clear=False,\n    )\n    if submission:\n        model.submit()\n    return model.cv_scores(), cv_data[OOF_TRAIN_NAME], cv_data[OOF_TEST_NAME]\n\ndef stack_score(members):\n    scores, _, _ = stack(TRAIN, TEST, members, verbose=0)\n    return scores[SCORE_NAME]","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-10-20T15:18:34.621491Z","iopub.execute_input":"2024-10-20T15:18:34.622162Z","iopub.status.idle":"2024-10-20T15:18:34.629155Z","shell.execute_reply.started":"2024-10-20T15:18:34.622117Z","shell.execute_reply":"2024-10-20T15:18:34.628126Z"}},"outputs":[],"execution_count":45},{"cell_type":"code","source":"","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-10-20T15:11:52.441238Z","iopub.execute_input":"2024-10-20T15:11:52.441911Z","iopub.status.idle":"2024-10-20T15:11:52.446681Z","shell.execute_reply.started":"2024-10-20T15:11:52.441871Z","shell.execute_reply":"2024-10-20T15:11:52.445679Z"}},"outputs":[],"execution_count":42},{"cell_type":"code","source":"","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-10-20T15:11:54.523078Z","iopub.execute_input":"2024-10-20T15:11:54.523963Z","iopub.status.idle":"2024-10-20T15:11:54.530238Z","shell.execute_reply.started":"2024-10-20T15:11:54.523921Z","shell.execute_reply":"2024-10-20T15:11:54.529354Z"}},"outputs":[{"execution_count":43,"output_type":"execute_result","data":{"text/plain":"array([[0.9957568 , 0.85315675, 0.9776696 ],\n       [0.0186219 , 0.06073099, 0.00834921],\n       [0.48351294, 0.29484564, 0.47396845],\n       ...,\n       [0.00410631, 0.03880844, 0.00897868],\n       [0.21473789, 0.4733675 , 0.37227377],\n       