alidation\n        y_val_pred_proba = model.predict_proba(X_valid)[:, 1]  # Probabilities for class 1 (loan approval)\n        val_auc = roc_auc_score(y_valid, y_val_pred_proba)\n        val_scores.append(val_auc)\n        print(f\"Validation AUC for fold {i + 1}: {val_auc:.4f}\")\n        \n        # Predict and evaluate\n        logits = model.predict_proba(test_data_LGBM)[:, 1]\n        test_preds[:, i] = logits\n        print(\"-\" * 50)\n        print(test_preds)\n        \n    # Average the predictions across the k folds\n    test_preds = np.mean(test_preds, axis=1)\n\n    # Return the final model (from the last fold) and the averaged predictions for the test set\n    print(f\"Average Validation AUC across folds: {np.mean(val_scores):.4f}\")\n    \n        \n    return model, test_preds","metadata":{"execution":{"iopub.status.busy":"2024-10-20T14:46:44.797410Z","iopub.execute_input":"2024-10-20T14:46:44.797682Z","iopub.status.idle":"2024-10-20T14:46:44.809692Z","shell.execute_reply.started":"2024-10-20T14:46:44.797651Z","shell.execute_reply":"2024-10-20T14:46:44.808806Z"},"trusted":true},"outputs":[],"execution_count":22},{"cell_type":"code","source":"LGBM_model, test_predsx = k_fold_lgbm(k=5, X_train=train_data_LGBM, y_train=train_labels_LGBM, model=model_lgbm)","metadata":{"execution":{"iopub.status.busy":"2024-10-20T14:46:44.810784Z","iopub.execute_input":"2024-10-20T14:46:44.811171Z","iopub.status.idle":"2024-10-20T14:47:48.922102Z","shell.execute_reply.started":"2024-10-20T14:46:44.811131Z","shell.execute_reply":"2024-10-20T14:47:48.921117Z"},"trusted":true},"outputs":[{"name":"stderr","text":"  0%|          | 0/5 [00:00<?, ?it/s]","output_type":"stream"},{"name":"stdout","text":"Training until validation scores don't improve for 200 rounds\n[150]\tvalid_0's binary_logloss: 0.204453\n[300]\tvalid_0's binary_logloss: 0.171665\n[450]\tvalid_0's binary_logloss: 0.161977\n[600]\tvalid_0's binary_logloss: 0.158414\n[750]\tvalid_0's binary_logloss: 0.1576\n[900